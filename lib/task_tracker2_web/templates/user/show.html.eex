<h1>Show User</h1>

<ul>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Fname:</strong>
    <%= @user.fname %>
  </li>

  <li>
    <strong>Lname:</strong>
    <%= @user.lname %>
  </li>

  <li>
    <strong>Admin:</strong>
    <%= @user.admin %>
